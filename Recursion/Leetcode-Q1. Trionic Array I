class Solution:
    def isTrionic(self, nums: List[int]) -> bool:
        n = len(nums)
        if n<3:
            return False
        def isincresing(num,start,end):
            for i in range(start,end):
                if num[i]>=num[i+1]:
                    return False
            return True
        def isdec(num,start,end):
            for i in range(start,end):
                if num[i]<=num[i+1]:
                    return False
            return True
        for i in range(1,n-2):
            if not isincresing(nums,0,i):
                continue
            for j in range(i+1,n-1):
                if isdec(nums,i,j) and isincresing(nums,j,n-1):
                    return True
        return False
        
