class Solution:
    def smallerNumbersThanCurrent(self, nums: List[int]) -> List[int]:
        output = []
        n = len(nums)
        for i in range(n):
            count = 0
            for j in range(0,n):
                if nums[j]<nums[i]:
                    count+=1
            output.append(count)
        return output
        
