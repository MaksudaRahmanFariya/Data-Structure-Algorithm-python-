class Solution:
    def matrixReshape(self, mat: List[List[int]], r: int, c: int) -> List[List[int]]:
        m, n = len(mat),len(mat[0])
        if m*n!=r*c:
            return mat
        if r==m and c == n:
            return mat
        rat = [[0 for _ in range(c)] for _ in range(r)]
        flot = []
        for i in range(m):
            for j  in range(n):
                flot.append(mat[i][j])
        k = 0
        for i in range(r):
            for j in range(c):
                rat[i][j] = flot[k]
                k+=1
        return rat
