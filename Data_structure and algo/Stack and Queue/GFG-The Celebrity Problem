from collections import Counter
class Solution:
    def celebrity(self, mat):
        # code here
        n = len(mat)
        if n == 1:
            return 0
        if n == 0:
            return -1
        celeb = [i for i in range(n)]
        while len(celeb)>1:
            a = celeb.pop()
            b = celeb.pop()
            if mat[a][b]==1:
                celeb.append(b)
            else:
                celeb.append(a)
        
        candidate = celeb.pop()
        for i in range(n):
            if candidate!=i:
                if mat[candidate][i] == 1 or mat[i][candidate]==0:
                    return -1
        return candidate
            
