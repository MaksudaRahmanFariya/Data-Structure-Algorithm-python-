# class ListNode:
#     def __init__(self, x):
#         self.val = x
#         self.next = None

class Solution:
    def detectCycle(self, head: Optional[ListNode]) -> Optional[ListNode]:
        visitedpos = set()
        itr = head
        while itr:
            if itr in visitedpos:
                return itr
            visitedpos.add(itr)
            itr = itr.next

        return None
