class Node:
    def __init__(self,val,next,prev):
        self.val = val
        self.prev = prev
        self.next = next
class MyLinkedList:

    def __init__(self):
        self.head = None
        #self.tail = None
        #self.size = 0
        
    def get(self, index: int) -> int:
        #if index<0 or index>=self.size:
            #return -1
        count = 0
        itr = self.head
        while itr:
            if count == index:
                #break
                return itr.val
            count +=1
            itr = itr.next
        return -1
    def addAtHead(self, val: int) -> None:
        node = Node(val,self.head,None)
        if self.head:
            self.head.prev = node
        self.head = node
        #self.head.prev = node.next
        #self.head = node
        #self.size +=1
    def addAtTail(self, val: int) -> None:
        if not self.head:
            self.head = Node(val,None,None)
        itr = self.head
        while itr.next:
            itr = itr.next
        node = Node(val,None,itr)
        itr.next = node
        #node = Node(val,None,self.tail)
        #if self.tail:
            #self.tail.next = node
        #else:
            #self.head = node
        #self.tail = node
        #size+=1
    def addAtIndex(self, index: int, val: int) -> None:
        #if index < 0 or index > self.size:
            #return
        if index == 0:
            self.addAtHead(val)
            return
        count = 0
        itr = self.head
        while itr:
            if count == index-1:
                next_node = itr.next
                node = Node(val,next_node,itr)
                itr.next = node
                if next_node:
                    next_node.prev = node
                return
                count+=1
                itr = itr.next
                i#tr.next.next.prev = itr.next.next
                #self.size +=1
    
    def deleteAtIndex(self, index: int) -> None:
        if index == 0 and self.head:
            self.head = self.head.next
            if self.head:
                self.head.prev = None
        count = 0
        itr = self.head
        while itr:
            if count == index:
                if itr.prev:
                    itr.prev.next = itr.next
                if itr.next:
                    itr.next.prev = itr.prev
                return

                #itr.next.next.prev = itr.next
                #itr.next = itr.next.next
                #self.size -=1
            count+=1
            itr = itr.next

