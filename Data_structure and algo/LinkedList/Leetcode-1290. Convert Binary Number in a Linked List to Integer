# Definition for singly-linked list.
# class ListNode:
#     def __init__(self, val=0, next=None):
#         self.val = val
#         self.next = next
class Solution:
    def getDecimalValue(self, head: Optional[ListNode]) -> int:
        itr = head
        res = ''
        while itr:
            res += str(itr.val)
            itr = itr.next
        n = len(res)
        output = 0
        for i in range(n):
            output += 2**(n-i-1)*(int(res[i]))
        return output
