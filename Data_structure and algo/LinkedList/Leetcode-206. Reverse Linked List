# Definition for singly-linked list.
# class ListNode:
#     def __init__(self, val=0, next=None):
#         self.val = val
#         self.next = next
class Solution:
    def reverseList(self, head: Optional[ListNode]) -> Optional[ListNode]:
        output = None
        itr = head
        while itr:
            new_node = ListNode(itr.val)
            new_node.next = output
            output = new_node
            itr = itr.next
        return output


        
