class Solution:
    def plusOne(self, digits: List[int]) -> List[int]:
        n = len(digits)
        carry = 1
        for i in range(n-1,-1,-1):
            new_val = (digits[i]+carry)%10
            carry = (digits[i]+carry)//10
            digits[i] = new_val
            if carry == 0:
                break
        if carry>0:
            digits.insert(0,carry)

        return digits
