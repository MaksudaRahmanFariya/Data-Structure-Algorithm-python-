class Solution:
    def isSubsequence(self, s: str, t: str) -> bool:
        m = len(s)
        n = len(t)
        j = 0
        i=0
        while j<m and i<n:
            if s[j]==t[i]:
                j=j+1
            i=i+1
        if(j==m):
            return True
        else:
            return False
        
            
        
